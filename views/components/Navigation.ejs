        <% navItems.forEach((item, index) => { %>
            <% if(index == 0) { %>
                <a class="navigation-list__item navigation-list__item-active">
                    <%= item %>
                </a>
            <% } else { %>
                <a class="navigation-list__item">
                    <%= item %>
                </a> 
            <% } %>
        <% }) %>   


